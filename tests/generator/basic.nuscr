timed global protocol Basic(role A, role B, role C) {
    Test1() from A to B within [0;1] using a and resetting ();
    Test1() from B to C within [0;1] using a and resetting ();
    Test1() from A to C within [0;1] using a and resetting ();
    Test1() from A to B within [0;1[ using a and resetting ();
    Test1(payload) from A to B within ]0;1[ using a and resetting ();
    Test2(integer) from A to B within [0;1] using a and resetting ();
    Test3(string) from A to B within [0;1] using a and resetting (a);
    Test4(payload) from A to B within [0;1] using a and resetting (a);
}
